{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "max-len": ["error", { "code": 120 }],
    "no-param-reassign": 0,
    "camelcase": 0,
    "no-use-before-define": 0,
    "dot-notation": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "no-else-return": 0,
    "strict": 0,
    "consistent-return": 0,
    "radix": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
    "arrow-body-style": 0
  }
}
